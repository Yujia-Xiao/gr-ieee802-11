<block>
  <name>Csma</name>
  <key>ieee802_11_csma</key>
  <category>IEEE802.11</category>
  <import>import ieee802_11</import>
  <make>ieee802_11.csma($threshold, $debug)</make>
  <param>
    <name>Threshold</name>
    <key>threshold</key>
    <type>float</type>
  </param>
  <param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	
	<sink>
		<name>in</name>
		<type>message</type>
		<optional>1</optional>
	</sink>

	<source>
		<name>out</name>
		<type>message</type>
		<optional>1</optional>
	</source>
	
</block>
